FRAMEWORK_SEARCH_PATHS = "${PODS_ROOT}/Sparkle" "$(PODS_ROOT)/Sparkle"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/ASCIImage" "${PODS_ROOT}/Headers/Public/CocoaAsyncSocket" "${PODS_ROOT}/Headers/Public/CocoaHTTPServer" "${PODS_ROOT}/Headers/Public/CocoaLumberjack" "${PODS_ROOT}/Headers/Public/Sparkle" "$(SDKROOT)/usr/include/libxml2"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/ASCIImage" -isystem "${PODS_ROOT}/Headers/Public/CocoaAsyncSocket" -isystem "${PODS_ROOT}/Headers/Public/CocoaHTTPServer" -isystem "${PODS_ROOT}/Headers/Public/CocoaLumberjack" -isystem "${PODS_ROOT}/Headers/Public/Sparkle"
OTHER_LDFLAGS = $(inherited) -ObjC -l"Pods-Hammerspoon-ASCIImage" -l"Pods-Hammerspoon-CocoaAsyncSocket" -l"Pods-Hammerspoon-CocoaHTTPServer" -l"Pods-Hammerspoon-CocoaLumberjack" -l"xml2" -framework "CoreServices" -framework "Security" -framework "Sparkle"
OTHER_LIBTOOLFLAGS = $(OTHER_LDFLAGS)
PODS_ROOT = ${SRCROOT}/Pods